#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules/node/bin/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules/node/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules/node/bin/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules/node/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node@20.19.4/node_modules:/Users/gspencer/code/genui/packages/spikes/gulf_server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../.pnpm/node@20.19.4/node_modules/node/bin/node"   "$@"
exit $?
